{
  "name": "grpc-nodejs-server",
  "version": "1.0.0",
  "description": "gRPC Node.js server for user service",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "generate-proto": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./generated --grpc_out=grpc_js:./generated --proto_path=./proto ./proto/user_service.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.5",
    "@grpc/proto-loader": "^0.7.8"
  },
  "devDependencies": {
    "grpc-tools": "^1.12.4"
  },
  "author": "Student",
  "license": "MIT"
}