{
  "name": "grpc-nodejs-client",
  "version": "1.0.0",
  "description": "gRPC Node.js client for user service",
  "main": "client.js",
  "scripts": {
    "generate": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./generated --grpc_out=grpc_js:./generated --proto_path=../../proto ../../proto/user_service.proto",
    "start": "node client.js",
    "test": "npm run generate && npm start"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.5",
    "@grpc/proto-loader": "^0.7.8"
  },
  "devDependencies": {
    "grpc-tools": "^1.12.4"
  },
  "author": "Student",
  "license": "MIT"
}